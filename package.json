{
  "name": "io-validate",
  "version": "2.2.0",
  "description": "Runtime data validator.",
  "repository": {
    "url": "https://github.com/yusangeng/io-validate.git"
  },
  "main": "index.js",
  "types": "/",
  "scripts": {
    "test": "nyc mocha --require babel-register test/*.spec.js",
    "coverage": "nyc report --reporter=lcov",
    "prepublishOnly": "npm run test",
    "ci": "npm run test && npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": {
    "name": "Y3G",
    "email": "yusangeng@outlook.com"
  },
  "keywords": [
    "validate",
    "validator",
    "validation"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "include": [
      "lib"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
